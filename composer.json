{
    "name": "temperworks/laravel_read_only_fields",
    "description": "This is my package ReadOnlyFields",
    "keywords": [
        "TemperWorks",
        "laravel",
        "laravel_read_only_fields"
    ],
    "homepage": "https://github.com/temperworks/laravel_read_only_fields",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Baelde",
            "email": "r.baelde@temper.works",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Temperworks\\ReadOnlyFields\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Temperworks\\ReadOnlyFields\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Temperworks\\ReadOnlyFields\\ReadOnlyFieldsServiceProvider"
            ],
            "aliases": {
                "ReadOnlyFields": "Temperworks\\ReadOnlyFields\\ReadOnlyFieldsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
